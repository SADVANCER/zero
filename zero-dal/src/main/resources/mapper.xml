<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="module">
    <resultMap id="module" type="dal.module.ModuleDO"/>
    <select id="selectAll" resultMap="module">
    select *
    from module
    </select>
    <select id="select" resultMap="module" parameterType="java.util.Map">
    SELECT *
    FROM module
    WHERE id = #{id}
  </select>
</mapper>
